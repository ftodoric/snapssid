{
  "manifest_version": 3,
  "name": "SnapSSID",
  "version": "1.0",
  "description": "Grab master.dev session token for locahost",
  "permissions": ["cookies", "tabs", "activeTab"],
  "host_permissions": [
    "https://master.dev.sofascore.dev/admin/*",
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "SnapSSID"
  },
  "icons": {
    "48": "icon.png"
  }
}
