{
  "manifest_version": 3,
  "name": "SnapSSID",
  "version": "1.0",
  "description": "Grab dev session token for localhost",
  "permissions": ["cookies", "tabs", "activeTab"],
  "host_permissions": ["<all_urls>", "http://localhost:3000/*"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "SnapSSID"
  },
  "icons": {
    "48": "icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "snapssid@example.com",
      "strict_min_version": "109.0"
    }
  }
}
